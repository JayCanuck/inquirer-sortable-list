{
  "name": "inquirer-sortable-list",
  "version": "1.0.0",
  "description": "Interactive commandline prompt for displaying and sorting a list",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaycanuck/inquirer-sortable-list.git"
  },
  "keywords": [
    "input",
    "inquire",
    "inquirer",
    "interface",
    "sort",
    "sortable",
    "reorderable",
    "list",
    "prompt",
    "cli",
    "command",
    "command-line",
    "stdin",
    "stdout",
    "terminal",
    "tty"
  ],
  "author": "Jason Robitaille",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JayCanuck/inquirer-sortable-list/issues"
  },
  "homepage": "https://github.com/JayCanuck/inquirer-sortable-list#readme",
  "files": [
    "dist"
  ],
